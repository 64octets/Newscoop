#!/bin/bash

. ../../../make.env

echo -n -e "\t\tWriting tpl_cgi.h header..."
cat > tpl_cgi.h << EOF
/******************************************************************************

Defines tpl_cgi defaults

WARNING!!! This file is generated. Do not edit!

******************************************************************************/

#ifndef _TPL_CGI_H
#define _TPL_CGI_H

#define ETC_DIR "$ETC_DIR"

#endif
EOF
[ $? -ne 0 ] && echo "ERROR" && exit 1
echo OK
